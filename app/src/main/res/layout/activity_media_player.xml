<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MediaPlayer.MediaPlayerActivity">

  <TextView android:id="@+id/tv_titel"
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
             android:text="Music Player"
             android:textSize="30dp"/>

    <SeekBar android:id="@+id/Sb_music"
             android:layout_width="match_parent"
             android:layout_height="57dp"
             android:progress="30"
             android:layout_below="@id/tv_titel" android:layout_alignParentStart="true"
             android:layout_marginTop="19dp"/>

  <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/Sb_music"
            android:id="@+id/txt" android:layout_alignParentStart="true" android:layout_marginTop="-4dp"
            android:layout_marginStart="11dp"/>
  <android.support.design.widget.FloatingActionButton
          android:id="@+id/fb_btn"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"

          android:layout_alignParentStart="true"
          android:layout_marginStart="148dp"
          android:layout_alignParentTop="true"
          android:layout_marginTop="165dp"/>


</RelativeLayout>